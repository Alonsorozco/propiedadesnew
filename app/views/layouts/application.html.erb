<!DOCTYPE html>
<html>
  <head>
    <title>Propiedades</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
       <p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>


<% if  user_signed_in? %>
<%= link_to 'Salir', destroy_user_session_path, method: :delete, class:"nav-link text-white" %>
<%else%>
      <%= link_to 'Registrate',new_user_registration_path, class:"nav-link text-dark" %>
       <%= link_to 'Inicia Sesion',user_session_path, class:"nav-link text-dark" %>
<% end %>
    <%= yield %>
  </body>
</html>
